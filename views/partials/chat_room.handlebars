<div id="chat_display">
	<ul id="messages_list">
		{{#each messages as |message|}}
			{{chat_message message me}}
		{{/each}}
	</ul>
</div>

<form id="new_message" action="">
	<input type="text" id="message_box" placeholder="Type message here...">
	<input type="hidden" id="message_sender" value="{{me}}">
	<input type="submit" id="new_message_submit" value="Send">
</form>